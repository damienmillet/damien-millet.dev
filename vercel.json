{
  "public": false,
  "routes": [
    { "src": "/", "dest": "/api/index.ts" },
    { "src": "/blog", "dest": "/api/blog.ts" },
    { "src": "/cheatsheets", "dest": "/api/cheatsheets.ts" }
  ],
  "functions": {
    "api/**/*.[jt]s": { "runtime": "vercel-deno@0.7.1" }
  },
  "build": {
    "env": {
      "DENO_TSCONFIG": "api/tsconfig.json"
    }
  }
}
